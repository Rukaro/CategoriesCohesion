{
  "version": 2,
  "builds": [
    {
      "src": "api/calculate.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/health.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/calculate-cohesion",
      "dest": "/api/calculate.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/calculate.py": {
      "maxDuration": 60
    },
    "api/health.py": {
      "maxDuration": 10
    }
  }
}


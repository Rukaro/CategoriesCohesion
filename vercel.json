{
  "version": 2,
  "routes": [
    {
      "src": "/api/calculate-cohesion",
      "dest": "/api/calculate.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/calculate.py": {
      "runtime": "python3.9",
      "maxDuration": 60
    },
    "api/health.py": {
      "runtime": "python3.9",
      "maxDuration": 10
    }
  }
}

